<launch>

    <!-- <env name="" value="" /> -->

    <!-- IMAGE SETTINGS -->
        <env name="IMAGE_HEIGHT" value="1080" />
        <env name="IMAGE_WIDTH" value="1920" />

    <!-- ROS NODE NAMES -->
        <!--MOTION PLANNING-->
        <env name="Core2MP_Req"  value="MotionPlanning_Request" />  <!--bool-->
        <env name="Core2MP_Data" value="MotionPlanning_Data" />     <!--Float32MultiArray-->
        <env name="MP2Core_Data" value="MotionPlanning_Progress" /> <!--Float32MultiArray-->

        <!--DETECTION-->
        <env name="Core2Det_Req" value="Detection_Request" /> <!--Bool-->
        <env name="Det2Core_Res" value="Detection_Result" />  <!--Float32MultiArray-->
        <env name="Det2Zed_Req"  value="Camera_Request" />    <!--Bool-->
        <env name="Zed2Det_Data" value="Camera_Data" />       <!--PointCloud2-->
        <env name="Zed2Det_Img"  value="Camera_Image" />      <!--Image-->

        <!--DOCKER MITM-->
        <env name="Docker_Data"  value="/ur5/zed_node/point_cloud/cloud_registered" /> <!--PointCloud2-->
        <env name="Docker_Image" value="/ur5/zed_node/left_raw/image_raw_color" />     <!--Image-->

</launch>